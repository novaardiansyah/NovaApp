{
  "name": "novaapp",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start --tunnel",
    "start:clean": "expo start --tunnel -c",
    "android": "expo run:android",
    "android:dev": "npx expo prebuild && npx expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:release": "npx expo prebuild && npx react-native build-android --mode=release",
    "submit:android": "eas build --platform android --auto-submit",
    "update:android": "eas update --channel production --platform android --message \"$npm_config_msg\""
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
